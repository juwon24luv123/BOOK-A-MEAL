{
    "check-coverage": true,
    "per-file": true,
    "lines": 99,
    "statements": 99,
    "functions": 99,
    "branches": 99,
    "require": ["babel-register"],
    "exclude":[
        "**/node_modules/**",
        "**/coverage/**",
        "**/test/**",
        "Ui-template"
    ],
    "reporter": ["lcov", "json", "text"],
    "sourceMap": false,
    "instrument": false,
    "cache": true,
    "report-dir": "./coverage"
    
}

